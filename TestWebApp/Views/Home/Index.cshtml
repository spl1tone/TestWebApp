@model List<TestWebApp.Models.Product>

<h2>Product List</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model) 
        {
            <tr>
                <td>@product.Id</td>
                <td>@product.Name</td>
                <td>@product.Price</td>
                <td>@product.Count</td>
            </tr>
        }
    </tbody>
</table>
